{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre"
    },
    "miniatura": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto",
      "mappedBy": "categoria"
    },
    "orden": {
      "type": "integer",
      "required": true,
      "unique": true
    }
  }
}
